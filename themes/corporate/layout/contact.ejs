<div class="container">
<ul class="breadcrumb">
  <li><a href="/">Trang chủ</a></li>
  <li class="active"><%- page.title %></li>
</ul>
<div class="main">
  <h1><%- page.title %></h1>
  <div class="row margin-bottom-40">
    <!-- BEGIN CONTENT -->
    <div class="col-md-12">
      <div class="content-page">
	<div class="row">
	  <div class="col-md-12">
	    <div id="map" class="gmaps margin-bottom-40" style="height:400px;"></div>
	  </div>
	  <div class="col-md-9 col-sm-9">
	    <%- page.content %>
	  </div>

	  <div class="col-md-3 col-sm-3 sidebar2">
	    <h2>Thông tin liên hệ</h2>
	    <address class="margin-bottom-40">
	      <%= theme.address_1 %><br>
	      <%= theme.address_2 %><br>
	      <%= theme.address_3 %><br>
	      Điện thoại: <%= theme.phone %><br>
	      Email: <a href="mailto:<%= theme.email %>"><%= theme.email %></a><br>
	      Skype: <a href="skype:<%= theme.skype %>"><%= theme.skype %></a>
	    </address>
	    <ul class="social-icons margin-bottom-40">
	      <% for(var s in theme.social) { %>
	      <li><a href="<%- url_for(theme.social[s]) %>"
	      data-original-title="<%= s %>" class="<%= s %>"></a></li>
	      <% } %>
	    </ul>  
	    <h2 class="padding-top-30">Về chúng tôi</h2>
	    <p>Tiệm khóa Tiến Phát</p>
	    <ul class="list-unstyled">
				<li><i class="fa fa-check"></i> Dịch vụ sửa khóa chuyên nghiệp</li>
				<li><i class="fa fa-check"></i> Giải pháp khóa an toàn</li>
	      <li><i class="fa fa-check"></i> Chìa khóa ô tô</li>
				<li><i class="fa fa-check"></i> Điều khiển cửa cuốn</li>
				<li><i class="fa fa-check"></i> Khóa chống trộm xe máy</li>
				<li><i class="fa fa-check"></i> Mở két sắt</li>
				<li><i class="fa fa-check"></i> Cung cấp khóa, chìa khóa và các vật tư ngành khóa</li>
	    </ul>        
	  </div>
	</div>
      </div>
    </div>
    <!-- END CONTENT -->
  </div>
</div>
</div>
